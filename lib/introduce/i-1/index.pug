section.canhcam-tabs-5(bg-img="./img/bg/bg-splendora.png")
	.container
		.row
			.col
				ul#ccTabs.nav.nav-tabs(role="tablist")
					li.nav-item
						a#cct-01-tab.nav-link(data-toggle="tab" href="#cct-01" role="tab" aria-controls="cct-01" aria-selected="true")
							span Tổng quan
					li.nav-item
						a#cct-02-tab.nav-link(data-toggle="tab" href="#cct-02" role="tab" aria-controls="cct-02" aria-selected="false")
							span Quy hoạch
					li.nav-item
						a#cct-03-tab.nav-link(data-toggle="tab" href="#cct-03" role="tab" aria-controls="cct-03" aria-selected="false")
							span Thiết kế
					li.nav-item
						a#cct-04-tab.nav-link(data-toggle="tab" href="#cct-04" role="tab" aria-controls="cct-04" aria-selected="false")
							span Các giai đoạn phát triển
				#ccTabsContent.tab-content
					#cct-01.tab-pane(role="tabpanel" aria-labelledby="cct-01-tab")
						include ../all-tab/tongquan/index.pug
					#cct-02.tab-pane(role="tabpanel" aria-labelledby="cct-02-tab") 
						include ../all-tab/quyhoach/index.pug
					#cct-03.tab-pane(role="tabpanel" aria-labelledby="cct-03-tab") 
						include ../all-tab/thietke/index.pug
					#cct-04.tab-pane(role="tabpanel" aria-labelledby="cct-04-tab") 
						include ../all-tab/phattrien/index.pug
